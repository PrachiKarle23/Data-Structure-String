Approach 1
class Solution {
public:
    void reverseString(vector<char>& s) {
       int i=0;
       int n=s.size()-1;
       while(i<=n){
           swap(s[i],s[n]);
           i++;
           n--;
       }
    }
};
Approach 2
class Solution {
public:
    void reverseString(vector<char>& s) {
       vector<int>a;
       int n=s.size();
       for(int i=n-1;i>=0;i--){
           a.push_back(s[i]);
       }
       for(int i=0;i<n;i++){
           s[i]=a[i];
       }
    }
};
