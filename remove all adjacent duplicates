class Solution {
public:
    string removeDuplicates(string s) {
        int i=1,n=s.length();
        while(i<n){
            if(s[i]==s[i-1]){
                s.erase(i-1,2);
                i=1;
            }
            else{
                i++;
            }
        }
        return s;
    }
};
